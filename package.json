{
  "name": "react-starter-template",
  "version": "1.2.0",
  "engines": {
    "node": ">= 18.x"
  },
  "type": "module",
  "description": "React starter template",
  "license": "MIT",
  "author": "Carlo Melisi <carlomelisi@gmail.com>",
  "scripts": {
    "prepare": "husky",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint \"{src,apps,libs,test}/**/*.{js,ts,jsx,tsx,mdx}\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.{js,ts,jsx,tsx,mdx}\" --fix",
    "lint:types": "tsc --noEmit",
    "format": "prettier --write \"./src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"./src/**/*.{ts,tsx}\"",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "test": "jest",
    "test:ci": "jest --ci",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "@hookform/resolvers": "3.9.1",
    "@tanstack/react-query": "5.62.3",
    "axios": "1.7.9",
    "clsx": "2.1.1",
    "lucide-react": "0.468.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "7.54.0",
    "react-router-dom": "7.0.2",
    "tailwind-merge": "2.5.5",
    "zod": "3.23.8",
    "zustand": "5.0.2"
  },
  "devDependencies": {
    "@eslint/compat": "1.2.4",
    "@eslint/eslintrc": "3.2.0",
    "@eslint/js": "9.16.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.1.0",
    "@testing-library/user-event": "14.5.2",
    "@types/jest": "29.5.14",
    "@types/node": "22.10.1",
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.1",
    "@typescript-eslint/eslint-plugin": "8.17.0",
    "@typescript-eslint/parser": "8.18.0",
    "@vitejs/plugin-react": "4.3.4",
    "autoprefixer": "10.4.20",
    "eslint": "9.16.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jest-dom": "5.5.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react": "7.37.2",
    "eslint-plugin-react-hooks": "5.1.0",
    "eslint-plugin-react-refresh": "0.4.16",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-testing-library": "7.1.1",
    "globals": "15.13.0",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-fixed-jsdom": "0.0.9",
    "jest-svg-transformer": "1.0.0",
    "jsdom": "25.0.1",
    "lint-staged": "15.2.10",
    "postcss": "8.4.49",
    "prettier": "3.4.2",
    "prettier-plugin-tailwindcss": "0.6.9",
    "rollup": "4.28.1",
    "tailwindcss": "3.4.16",
    "ts-jest": "29.2.5",
    "typescript": "5.7.2",
    "util": "0.12.5",
    "vite": "6.0.3",
    "vite-tsconfig-paths": "5.1.4",
    "whatwg-fetch": "3.6.20"
  }
}
